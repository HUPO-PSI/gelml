		<ElectrophoresisProtocol
			name="1st dimension electrophoresis protocol (enter optional other name here)"
			identifier="ex003:ElectrophoresisProtocol0">
			<ElectrophoresisStep identifier="ex003:ElectrophoresisStep0" actionOrdinal="1">
				<ElectricalParameter identifier="ex003:ElectricalParameter0">
					<fuge:AtomicValue value="500">
						<fuge:_unit OntologyTerm_ref="ex003:OntologyIndividual1"/>
					</fuge:AtomicValue>
					<_electricalSetting OntologyTerm_ref="ex003:OntologyIndividual18"/>			

				</ElectricalParameter>

				<Duration identifier="ex003:Duration0">
					<fuge:AtomicValue value="1">
						<fuge:_unit OntologyTerm_ref="ex003:OntologyIndividual0"/>
					</fuge:AtomicValue>
				</Duration>

			</ElectrophoresisStep>

			<ElectrophoresisStep identifier="ex003:ElectrophoresisStep4">
				<ElectricalParameter identifier="ex003:ElectricalParameter2">
					<fuge:AtomicValue value="1000">
						<fuge:_unit OntologyTerm_ref="ex003:OntologyIndividual1"/>
					</fuge:AtomicValue>
					<_electricalSetting OntologyTerm_ref="ex003:OntologyIndividual19"/>

				</ElectricalParameter>

				<Duration identifier="ex003:Duration2">
					<fuge:AtomicValue value="1">
						<fuge:_unit OntologyTerm_ref="ex003:OntologyIndividual0"/>
					</fuge:AtomicValue>

				</Duration>

			</ElectrophoresisStep>

			<ElectrophoresisStep identifier="ex003:ElectrophoresisStep5" actionOrdinal="3">
				<ElectricalParameter identifier="ex003:ElectricalParameter3">
					<fuge:AtomicValue value="8000">
						<fuge:_unit OntologyTerm_ref="ex003:OntologyIndividual1"/>
					</fuge:AtomicValue>

					<_electricalSetting OntologyTerm_ref="ex003:OntologyIndividual18"/>

				</ElectricalParameter>

				<Duration identifier="ex003:Duration3">
					<fuge:AtomicValue value="0.034">
						<fuge:_unit OntologyTerm_ref="ex003:OntologyIndividual5"/>
					</fuge:AtomicValue>
				</Duration>

			</ElectrophoresisStep>

		</ElectrophoresisProtocol>

		<ElectrophoresisProtocol
			name="2nd dimension electrophoresis protocol (enter optional other name here)"
			identifier="ex003:ElectrophoresisProtocol1">
			<_electrophoresisEquipment GenericEquipment_ref="ex003:GenericEquipment0"/>

